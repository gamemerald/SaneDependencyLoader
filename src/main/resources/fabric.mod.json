{
  "schemaVersion": 1,
  "id": "sanedeploader",
  "version": "${version}",

  "name": "Sane Dependency Loader",
  "description": "A Crap dependency loader (Downloader) mod!",
  "authors": [
    "Gamemerald"
  ],
  "contact": {
    "homepage": "https://github.com/gamemerald",
    "issues":  "https://github.com/gamemerald/SaneDependencyLoader/issues",
    "sources": "https://github.com/gamemerald/SaneDependencyLoader"
  },

  "license": "CC0-1.0",
  "icon": "assets/sanedeploader/icon.png",

  "environment": "*",
  "entrypoints": {
    "preLaunch": [
      "io.gamemerald.sanedependencyloader.SaneDepLoader"
    ],
    "main": [
      "io.gamemerald.sanedependencyloader.LaunchedStage"
    ]
  },
  "mixins": [
    "sanedeploader.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "minecraft": "~1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
